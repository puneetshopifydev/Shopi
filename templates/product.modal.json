{
  "sections": {
    "main": {
      "type": "main-product-modal",
      "blocks": {
        "title": {
          "type": "title",
          "settings": {
            "title_size": "h2"
          }
        },
        "rating": {
          "type": "rating",
          "settings": {}
        },
        "price": {
          "type": "price",
          "settings": {}
        },
        "subtitle": {
          "type": "text",
          "settings": {
            "icon": "none",
            "text": "{{ product.metafields.descriptors.subtitle.value }}",
            "text_style": "subtitle"
          }
        },
        "read_more": {
          "type": "read_more",
          "settings": {}
        },
        "variant_picker": {
          "type": "variant_picker",
          "settings": {
            "show_variant_labels": true,
            "picker_type": "button",
            "enable_color_swatches": true,
            "swatch_type": "round"
          }
        },
        "inventory_status": {
          "type": "inventory_status",
          "settings": {
            "inventory_threshold": 10,
            "show_inventory_transfer": true
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_quantity_selector": true,
            "show_dynamic_checkout": true
          }
        },
        "share": {
          "type": "share",
          "settings": {}
        }
      },
      "block_order": [
        "title",
        "rating",
        "price",
        "subtitle",
        "read_more",
        "variant_picker",
        "inventory_status",
        "buy_buttons",
        "share"
      ],
      "settings": {
        "hide_variants": true,
        "enable_video_looping": false,
        "enable_image_fill": true,
        "mobile_image_ratio": "adapt_first"
      }
    }
  },
  "order": [
    "main"
  ]
}